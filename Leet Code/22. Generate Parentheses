/**
* 22. Generate Parentheses
* Given n pairs of parentheses, write a function to generate all combinations of well-formed parentheses
*/

class Solution {
    public void gen( String s, int open, int close,List<String> output, int n) {
        if(s.length() == 2*n) {
            output.add(s);
            return;
        }   
        if(open > 0) {
            gen(s+"(",open-1,close,output,n);
        }
        if(close>open) {
            gen(s+")",open,close-1,output,n);
        }
    }
    public List<String> generateParenthesis(int n) {
        String s = "(";
        List<String> output = new LinkedList<>();
        gen(s,n-1,n,output,n);
        return output;
    }
}